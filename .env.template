# WandB API Key (발급받은 실제 키를 입력하세요)
WANDB_API_KEY=your_wandb_api_key_here

# WandB Project Settings  
WANDB_PROJECT=cv-classification-team
# WANDB_ENTITY=your_entity_name  # 필요시 팀 entity로 변경

# Optional Settings
WANDB_MODE=online
WANDB_TAGS=cv,classification,document,upstage

# 사용법:
# 1. 이 파일을 .env로 복사: cp .env.template .env
# 2. your_wandb_api_key_here를 실제 API 키로 변경
# 3. 필요시 WANDB_PROJECT와 WANDB_ENTITY 수정